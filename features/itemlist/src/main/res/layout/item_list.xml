<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="386dp"
        android:layout_height="211dp"
        android:layout_marginTop="32dp"
        app:cardBackgroundColor="#991EE2E9"
        app:cardCornerRadius="8dp"
        app:cardElevation="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/tvDesc5"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="Id"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDesc7"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="Nombre"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDesc2"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="@string/tvDescription"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDesc11"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="Es taxable"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDesc3"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="Tipo"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDesc4"
                    android:layout_width="94dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:text="Tasa"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/tvIdCard"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvNameCard"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvDescriptionCard"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvIsTaxable"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvTypeCard"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

                <TextView
                    android:id="@+id/tvRateCard"
                    android:layout_width="255dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_condensed_bold"
                    android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                    tools:ignore="UnknownId" />

            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>